//Write a program to store the data of 2 students.
#include<stdio.h>
#include<string.h>
struct student{
    int roll;
    float cgpa;
    char name[100];
};
int main(){
    struct student s1;
    s1.roll=1;
    s1.cgpa=3.8;
    strcpy(s1.name,"rohit");
    printf("%s\n%d\n%.2f\n\n",s1.name,s1.roll,s1.cgpa);
    struct student s2;
    s2.roll=2;
    s2.cgpa=3.5;
    strcpy(s2.name,"hasan");
    printf("%s\n%d\n%.2f\n",s2.name,s2.roll,s2.cgpa);
    return 0;
}
