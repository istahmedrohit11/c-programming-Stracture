/*
Passing structure to function
//Function Prototype
void printInfo(struct student s1);
*/
#include<stdio.h>
#include<string.h>
struct student{
    int roll;
    float cgpa;
    char name[100];
};
void printInfo(struct student s1){
    printf("Roll = %d\n",s1.roll);
    printf("Name = %s\n",s1.name);
    printf("Cgpa = %f\n",s1.cgpa);
}
int main(){
    struct student s1={1664 , 3.4, "Rohit" };
    printInfo(s1);
    return 0;
}
